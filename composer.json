{
  "name": "gdbots/app-bundle",
  "homepage": "https://github.com/gdbots/app-bundle",
  "description": "App bundle for Symfony apps.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.4",
    "symfony/console": "^7.2 || ^8.0",
    "symfony/framework-bundle": "^7.2 || ^8.0",
    "twig/twig": "^3.20"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\AppBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\AppBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
