{
  "name": "gdbots/app-bundle",
  "homepage": "https://github.com/gdbots/app-bundle",
  "description": "App bundle for symfony apps.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "symfony/symfony": "~3.3"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\AppBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\AppBundle\\": "tests"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Gdbots\\Bundle\\AppBundle\\Composer\\ScriptHandler::installConstantsFile"
    ],
    "post-update-cmd": [
      "Gdbots\\Bundle\\AppBundle\\Composer\\ScriptHandler::installConstantsFile"
    ],
    "test": "vendor/bin/phpunit"
  }
}
