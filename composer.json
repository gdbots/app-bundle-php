{
  "name": "gdbots/app-bundle",
  "homepage": "https://github.com/gdbots/app-bundle",
  "description": "App bundle for Symfony apps.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "symfony/console": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "composer/composer": "dev-master",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\AppBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\AppBundle\\": "tests"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Gdbots\\Bundle\\AppBundle\\Composer\\ScriptHandler::installConstantsFile"
    ],
    "post-update-cmd": [
      "Gdbots\\Bundle\\AppBundle\\Composer\\ScriptHandler::installConstantsFile"
    ],
    "test": "vendor/bin/phpunit"
  }
}
