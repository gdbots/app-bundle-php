{
  "name": "gdbots/app-bundle",
  "homepage": "https://github.com/gdbots/app-bundle",
  "description": "App bundle for Symfony apps.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.2",
    "symfony/console": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "twig/twig": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\AppBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\AppBundle\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
